Function StrConvFromUnicode(sInp)
	Dim nCnt
	Dim nLen
	Dim nAsc
	nLen = Len(sInp)
	For nCnt = 1 to nLen
		nAsc = Asc(Mid(sInp, nCnt, 1))
		If nAsc And &hFF00 Then
			StrConvFromUnicode = StrConvFromUnicode & ChrB(Int(nAsc / 256) And &hFF)
			StrConvFromUnicode = StrConvFromUnicode & ChrB(nAsc And &hFF)
		Else
			StrConvFromUnicode = StrConvFromUnicode & ChrB(nAsc)
		End If
	Next
End Function

'***************************************************
' StrConv Ç™égópÇ∑ÇÈíËêîåS
'***************************************************
' Enum VbStrConv
Const vbUpperCase=1
Const vbLowerCase=2
Const vbProperCase=3
Const vbWide=4
Const vbNarrow=8
Const vbKatakana=16
Const vbHiragana=32
Const vbUnicode = 64
Const vbFromUnicode = 128

'***************************************************
' StrConv
'---------------------------------------------------
' à¯êî : ïœä∑Ç∑ÇÈï∂éöóÒ,ïœä∑èàóù
' ñﬂíl : ïœä∑Ç≥ÇÍÇΩï∂éöóÒ
'***************************************************
Function StrConv(sInp,eCnv)
	StrConv = sInp
	' Cnv Ç…ëŒÇµÇƒèàóùÇêUÇËï™ÇØ
	If eCnv And vbUpperCase Then
		StrConv = StrConvUpperCase(StrConv)
	End If
	If eCnv And vbLowerCase Then
		StrConv = StrConvLowerCase(StrConv)
	End If
	If eCnv = vbProperCase Then
		StrConv = StrConvProperCase(StrConv)
	End If
	If eCnv And vbWide Then
		StrConv = StrConvWide(StrConv)
	End If
	If eCnv And vbNarrow Then
		StrConv = StrConvNarrow(StrConv)
	End If
	If eCnv And vbKatakana Then
		StrConv = StrConvKatakana(StrConv)
	End If
	If eCnv And vbHiragana Then
		StrConv = StrConvHiragana(StrConv)
	End If
	If eCnv And vbUnicode Then
		StrConv = StrConvUnicode(StrConv)
	End If
	If eCnv And vbFromUnicode Then
		StrConv = StrConvFromUnicode(StrConv)
	End If
End Function


Function FileListing( objFolder )
	'WScript.Echo LenB( StrConvFromUnicode( objFolder.Path ) ) & " : " & objFolder.Path & "\"
	If LenB( StrConvFromUnicode( objFolder.Path ) ) > 240 Then
		WScript.Echo "240 OVER!! " & LenB( StrConvFromUnicode( objFolder.Path ) ) & " : " & objFolder.Path & "\"
		Exit Function
	End If

	For Each objSubFolder In objFolder.SubFolders
		FileListing( objSubFolder )
	Next

	For Each objFile In objFolder.Files
		'WScript.Echo LenB( StrConvFromUnicode( objFile.Path ) ) & " : " & objFile.Path
		If LenB( StrConvFromUnicode( objFile.Path ) ) > 240 Then
			WScript.Echo "240 OVER!! " & LenB( StrConvFromUnicode( objFile.Path ) ) & " : " & objFile.Path
			WScript.Echo "240 OVER!!"
			WScript.Quit
		End If
	Next
End Function


Set objFS = WScript.CreateObject( "Scripting.FileSystemObject" )

FileListing( objFS.GetFolder( WScript.Arguments.Item(0) ) )

